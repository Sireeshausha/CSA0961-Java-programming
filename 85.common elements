public class Commonele {
    public static void main(String[] args) {
        int[] arr1 = {2,3,5,10,1};
        int[] arr2 = {2, 4, 3, 6, 1};

        for (int i = 0; i < arr1.length; i++) {
            for (int j = 0; j < arr2.length; j++) {
                if (arr1[i] == arr2[j]) {
                    System.out.print(arr1[i] + " ");
                }
            }
        }
    }
}
